{
  "tasks": {
    "start": "deno run --allow-read --allow-write --allow-net --allow-env --allow-run --unstable-kv --watch=static/,routes/ main.ts",
    "build": "deno run --allow-read --allow-write --allow-net --allow-env --allow-run --unstable-kv dev.ts build",
    "preview": "deno run --allow-read --allow-write --allow-net --allow-env --unstable-kv main.ts"
  },
  "nodeModulesDir":"auto",
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "$std/": "https://deno.land/std@0.208.0/",
    "@snowplow/node-tracker": "npm:@snowplow/node-tracker"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"]
}
